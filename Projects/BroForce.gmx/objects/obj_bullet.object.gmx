        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Update stuffs

// If currently colliding die
if( place_meeting( x, y, obj_ground ) ) {
    instance_destroy();
    exit;
}

// Previous speeds
phspd = hspd;
pvspd = vspd;

// Facing where you're going
if( hspd != 0 ) {
    facing = sign(hspd);
}

// Default face right
if( facing == 0 ) {
    facing = 1;
}

// Horizontal movement
h = round(hspd);
hdebt += h - hspd;
if( abs(hdebt) &gt;= 1 ) {
    h -= sign(hdebt);
    hdebt -= sign(hdebt);
}
// If you will run into a wall
if( place_meeting( x + h, y, obj_ground ) ) {
    // Move to the wall
    while( !place_meeting( x + sign(h), y, obj_ground ) ) {
        x += sign(h);
    }
    // Apply damage to the wall you hit
    var wall = instance_place( x + sign(h), y, obj_ground );
    wall.dmg = dmg;
    with( wall ) {
        hth = max( 0, hth - dmg );
    }
    // Destroy
    instance_destroy();
    exit;
}
x += h;

// Vertical movement
v = round(vspd);
vdebt += v - vspd;
if( abs(vdebt) &gt;= 1 ) {
    v -= sign(vdebt);
    vdebt -= sign(vdebt);
}
// If you will run into a wall
if( place_meeting( x, y + v, obj_ground ) ) {
    // Move to the wall
    while( !place_meeting( x, y + sign(v), obj_ground ) ) {
        y += sign(v);
    }
    // Destroy
    instance_destroy();
    exit;
}
y += v;

// If going into the ground, destroy
if( vspd &gt;= 0 &amp;&amp; place_meeting( x, y+1, obj_ground ) ) {
    instance_destroy();
    exit;
}

// Slow to nothing when close to zero
if( abs( hspd ) &lt;= tooslow ) {
    hspd = 0;
}
if( abs( vspd ) &lt;= tooslow ) {
    vspd = 0;
}

// Die after stopped a bit
if( x == xprevious &amp;&amp; y == yprevious ) {
    dtime += 1 / room_speed;
    if( dtime &gt;= ddead ) {
        instance_destroy();
        exit;
    }
} else {
    dtime = 0;
}

// Die after a certain distance from your starting point
// Only if you provided one...
if( ddist != -1 ) {
    if( abs( x - xstr ) &gt;= ddist ) {
        instance_destroy();
        exit;
    }
}
</string>
