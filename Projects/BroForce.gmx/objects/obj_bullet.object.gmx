        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Update stuffs

// If currently colliding die
if( place_meeting( x, y, obj_ground ) ) {
    instance_destroy();
    exit;
}

// Previous speeds
phspd = hspd;
pvspd = vspd;

// Falling?
if( vspd &gt;= 0 &amp;&amp; place_meeting( x, y+1, obj_ground ) ) {
    onground = true;
    vspd = 0;
    vdebt = 0;
} else {
    onground = false;
    vspd += grav;
    if( vspd &gt; vmax ) {
        vspd = vmax;
    }
    if( vspd &gt; 0 &amp;&amp; place_meeting( x, y+1, obj_ground ) ) {
        vspd = 0;
    }
}

// Facing where you're going
if( hspd != 0 ) {
    facing = sign(hspd);
}

// Default face right
if( facing == 0 ) {
    facing = 1;
}

// Horizontal movement
h = round(hspd);
hdebt += h - hspd;
if( abs(hdebt) &gt;= 1 ) {
    h -= sign(hdebt);
    hdebt -= sign(hdebt);
}
if( place_meeting( x + h, y, obj_ground ) ) {
    while( !place_meeting( x + sign(h), y, obj_ground ) ) {
        x += sign(h);
    }
    hspd = 0;
    h = 0;
    hdebt = 0;
}
x += h;

// Hit a wall, stick if fast
if( hspd == 0 &amp;&amp; phspd != 0 ) {
    var face = facing;
    if( abs( phspd ) &gt;= impactspd ) {
        instance_change( obj_impact, true );
        facing = face;
    } else {
        var spdh = -phspd * hbounce;
        var spdv = vspd;
        instance_change( obj_shell, true );
        facing = face;
        hspd = spdh;
        vspd = spdv;
    }
    exit;
}

// Vertical movement
v = round(vspd);
vdebt += v - vspd;
if( abs(vdebt) &gt;= 1 ) {
    v -= sign(vdebt);
    vdebt -= sign(vdebt);
}
if( place_meeting( x, y + v, obj_ground ) ) {
    while( !place_meeting( x, y + sign(v), obj_ground ) ) {
        y += sign(v);
    }
    vspd = 0;
    v = 0;
    vdebt = 0;
}
y += v;

// Hit ground or up, change to shell when slow
if( ( (vspd == 0 &amp;&amp; pvspd != 0) ||
        (vspd &gt;= 0 &amp;&amp; place_meeting( x, y+1, obj_ground )) ) &amp;&amp;
        abs( hspd ) &lt;= shellspd ) {
    var face = facing;
    var spdh = hspd;
    var spdv = -pvspd * vbounce;
    instance_change( obj_shell, true );
    facing = face;
    hspd = spdh;
    vspd = spdv;
    exit;
}

// Update ground
onground = place_meeting( x, y+1, obj_ground );

// Slow to nothing when close to zero
if( abs( hspd ) &lt;= tooslow ) {
    hspd = 0;
}
if( onground &amp;&amp; abs( vspd ) &lt;= tooslow ) {
    vspd = 0;
}

// Friction on ground
if( onground &amp;&amp; vspd &gt;= 0 ) {
    hspd = hspd * (1 - frict);
}

// Die after stopped a bit
if( x == xprevious &amp;&amp; y == yprevious ) {
    if( fps != 0 ) {
        dtime += 1 / fps;
    }
    if( dtime &gt;= ddead ) {
        instance_destroy();
    }
} else {
    dtime = 0;
}
</string>
          </argument>
