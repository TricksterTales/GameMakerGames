        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Update stuffs

// If currently colliding die
if( place_meeting( x, y, obj_ground ) ) {
    instance_destroy();
    exit;
}

// Falling?
if( place_meeting( x, y+1, obj_ground ) ) {
    onground = true;
    vspd = 0;
    vdebt = 0;
} else {
    onground = false;
    vspd += grav;
    if( vspd &gt; vmax ) {
        vspd = vmax;
    }
}

// Facing where you're going
if( hspd != 0 ) {
    facing = sign(hspd);
}

// Default face right
if( facing == 0 ) {
    facing = 1;
}

// Horizontal movement
h = round(hspd);
hdebt += h - hspd;
if( abs(hdebt) &gt;= 1 ) {
    h -= sign(hdebt);
    hdebt -= sign(hdebt);
}
if( place_meeting( x + h, y, obj_ground ) ) {
    while( !place_meeting( x + sign(h), y, obj_ground ) ) {
        x += sign(h);
    }
    hspd = 0;
    h = 0;
    hdebt = 0;
}
x += h;

// Vertical movement
v = round(vspd);
vdebt += v - vspd;
if( abs(vdebt) &gt;= 1 ) {
    v -= sign(vdebt);
    vdebt -= sign(vdebt);
}
if( place_meeting( x, y + v, obj_ground ) ) {
    while( !place_meeting( x, y + sign(v), obj_ground ) ) {
        y += sign(v);
    }
    vspd = 0;
    v = 0;
    vdebt = 0;
}
y += v;

// Update ground
onground = place_meeting( x, y+1, obj_ground );
</string>
