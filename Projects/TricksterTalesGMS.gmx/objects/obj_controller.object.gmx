          <argument>
            <kind>1</kind>
            <string>/// Maximize fps

// Reduce number of balls
if( instance_number( obj_ball ) &gt; max_ball ) {
    var num = instance_number( obj_ball );
    var delete = num - max_ball;
    var obj = noone;
    var counter = 0;
    while( delete &gt; 0 ) {
        delete--;
        obj = instance_find( obj_ball, counter );
        if( instance_exists( obj ) ) {
            with( obj ) {
                instance_destroy();
                num--;
            }
            obj = -1;
        } else {
            counter++;
            delete++;
        }
        if( counter &gt;= num ) {
            break;
        }
    }
}
