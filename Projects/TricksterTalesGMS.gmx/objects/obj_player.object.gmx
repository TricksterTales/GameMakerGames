          <argument>
            <kind>1</kind>
            <string>/// Update stuffs

var k_left = keyboard_check(vk_left);
var k_right = keyboard_check(vk_right);
var k_up = keyboard_check(vk_up);
var k_down = keyboard_check(vk_down);

// Falling or jumping?
if( place_meeting( x, y+1, obj_ground ) ) {
    vspd = 0;
    vdebt = 0;
    if( k_up &amp;&amp; !k_down ) {
        vspd = -jspd;
    }
} else {
    vspd += grav;
    if( vspd &gt; vmax ) {
        vspd = vmax;
    }
}

// Side to side
if( k_left xor k_right ) {
    if( k_left ) {
        hspd = -wspd;
    }
    if( k_right ) {
        hspd = wspd;
    }
} else {
    hspd = 0;
}

// Horizontal movement
var h = round(hspd);
hdebt += h - hspd;
if( abs(hdebt) &gt;= 1 ) {
    h -= sign(hdebt);
    hdebt -= sign(hdebt);
}
if( place_meeting( x + h, y, obj_ground ) ) {
    while( !place_meeting( x + sign(h), y, obj_ground ) ) {
        x += sign(h);
    }
    hspd = 0;
    h = 0;
    hdebt = 0;
}
x += h;

// Vertical movement
var v = round(vspd);
vdebt += v - vspd;
if( abs(vdebt) &gt;= 1 ) {
    v -= sign(vdebt);
    vdebt -= sign(vdebt);
}
if( place_meeting( x, y + v, obj_ground ) ) {
    while( !place_meeting( x, y + sign(v), obj_ground ) ) {
        y += sign(v);
    }
    vspd = 0;
    v = 0;
    vdebt = 0;
}
y += v;
</string>
